{
  "index_patterns": [
    "zeebe-record-*"
  ],
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 1
  },
  "mappings": {
    "_doc": {
      "dynamic": "strict",
      "properties": {
        "position": {
          "type": "long"
        },
        "raftTerm": {
          "type": "integer"
        },
        "sourceRecordPosition": {
          "type": "long"
        },
        "producerId": {
          "type": "integer"
        },
        "key": {
          "type": "long"
        },
        "timestamp": {
          "type": "date",
          "format": "epoch_millis"
        },
        "intent": {
          "type": "keyword"
        },
        "partitionId": {
          "type": "integer"
        },
        "recordType": {
          "type": "keyword"
        },
        "rejectionType": {
          "type": "keyword"
        },
        "rejectionReason": {
          "type": "text"
        },
        "valueType": {
          "type": "keyword"
        },
        "value": {
          "type": "text"
        }
      }
    }
  }
}
